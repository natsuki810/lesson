package lesson;

public class Main {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int buttonCode = 3;
		switch (buttonCode) {
		case 1:
			System.out.println("A");
			break;
		case 2:
			System.out.println("B");
			break;
		case 3:
			System.out.println("X");
			break;
		case 4:
			System.out.println("Y");
			break;
		default:
			System.out.println("不明");
			break;
		}
	}
}
